{
  "name": "metamask-docs",
  "version": "1.0.0",
  "private": true,
  "license": "(MIT OR Apache-2.0)",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start -p 3003",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "typecheck-staged": "tsc-files --noEmit",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint . --max-warnings=5",
    "lint:style": "stylelint 'src/**/*.css' --fix",
    "lint:fix": "npm run lint:js -- --fix",
    "format": "prettier --write '{blog,docs,src}/**/*.{md,mdx,ts,js,tsx,jsx,json}'"
  },
  "lint-staged": {
    "src/**/*.{ts,js,jsx,tsx}": "npm run lint:fix",
    "**/*.{ts,tsx}": "npm run typecheck-staged",
    "**/*.css": "npm run lint:style",
    "**/*.{md,mdx,ts,js,tsx,jsx,json}": "npm run format"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^14.1.1",
    "@docusaurus/core": "3.8.1",
    "@docusaurus/plugin-content-docs": "3.8.1",
    "@docusaurus/plugin-content-pages": "3.8.1",
    "@docusaurus/plugin-google-gtag": "^3.8.1",
    "@docusaurus/plugin-google-tag-manager": "^3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@docusaurus/remark-plugin-npm2yarn": "^3.8.1",
    "@docusaurus/theme-common": "3.8.1",
    "@docusaurus/theme-mermaid": "3.8.1",
    "@intercom/messenger-js-sdk": "^0.0.14",
    "@mdx-js/react": "^3.1.0",
    "@metamask/design-tokens": "^8.0.0",
    "@metamask/eth-sig-util": "^8.2.0",
    "@metamask/profile-sync-controller": "^16.0.0",
    "@metamask/sdk": "^0.33.0",
    "@rjsf/core": "^5.24.12",
    "@rjsf/utils": "^5.24.12",
    "@rjsf/validator-ajv8": "^5.24.12",
    "@sentry/browser": "^9.39.0",
    "@types/react": "^19.1.8",
    "clsx": "^2.1.1",
    "docusaurus-plugin-sass": "^0.2.6",
    "dotenv": "^17.2.0",
    "ethers": "^6.15.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "launchdarkly-js-client-sdk": "^3.8.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isplainobject": "^4.0.6",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "prettier": "^3.6.2",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-alert": "^7.0.3",
    "react-dom": "^19.1.0",
    "react-dropdown-select": "^4.12.2",
    "react-modal": "^3.16.3",
    "react-player": "^3.3.1",
    "react-tippy": "^1.4.0",
    "remark-codesandbox": "^0.10.1",
    "remark-docusaurus-tabs": "^0.2.0",
    "sass": "^1.89.2"
  },
  "devDependencies": {
    "@docusaurus/eslint-plugin": "3.8.1",
    "@docusaurus/module-type-aliases": "3.8.1",
    "@docusaurus/plugin-client-redirects": "3.8.1",
    "@docusaurus/tsconfig": "3.8.1",
    "@docusaurus/types": "3.8.1",
    "@eslint/js": "^9.31.0",
    "@tsconfig/docusaurus": "^2.0.3",
    "@types/react-alert": "^7.0.6",
    "@types/react-modal": "^3.16.3",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react": "^7.37.5",
    "stylelint": "^16.21.1",
    "stylelint-config-standard": "^38.0.0",
    "tsc-files": "^1.1.4",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "react-alert": {
      "react": "^19.1.0",
      "react-dom": "^19.1.0"
    }
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "22.x"
  }
}
